programa {
  funcao inicio() {
    real numero1, numero2, numero3, numero4, numero5, numero6, soma

    escreva("Digite o primeiro número: ")
    leia(numero1)

    escreva("Digite o segundo número: ")
    leia(numero2)

    escreva("Digite o terceiro número: ")
    leia(numero3)

    escreva("Digite o quarto número: ")
    leia(numero4)

    escreva("Digite o quinto número: ")
    leia(numero5)

    escreva("Digite o sexto número: ")
    leia(numero6)

    soma = 0

    se (numero1 < 72){
      soma = numero1 + soma
    }
    se (numero2 < 72){
      soma = numero2 + soma
    }
    se (numero3 < 72){
      soma = numero3 + soma
    }
    se (numero4 < 72){
      soma = numero4 + soma
    }
    se (numero5 < 72){
      soma = numero5 + soma
    }
    se (numero6 < 72){
      soma = numero6 + soma
    }

    escreva("Os números informados são em ordem: ", numero1 , numero2 , numero3 , numero4 , numero5 , numero6 , ". A soma é ", soma)
    }
  }
