programa {
  funcao inicio() {
      real numero1, numero2, numero3, numero4, numero5, numero6, soma, divisao

      escreva("Digite o primeiro número: ")
      leia(numero1)

      escreva("Digite o segundo número: ")
      leia(numero2)

      escreva("Digite o terceiro número: ")
      leia(numero3)

      escreva("Digite o quarto número: ")
      leia(numero4)

      escreva("Digite o quinto número: ")
      leia(numero5)

      escreva("Digite o sexto número: ")
      leia(numero6)

      soma = numero1 + numero2 + numero3 + numero4 + numero5 + numero6
      divisao = soma / 6

      escreva("A média artimética dos números é ", divisao)
  }
}
